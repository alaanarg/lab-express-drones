<h2>Available drones</h2>
<hr />
{{#each allDrones}}

  <h3>{{this.name}}</h3>
    Propellers: {{this.propellers}}
    <br>
    Max Speed: {{this.maxSpeed}}
    <br><br>  
    <a href="/drones/{{this._id}}/edit">Update Drone</a>
    <br><br>
    <form method="post" action="/drones/{{this._id}}/delete">
    <button type="submit">Delete drone</button>
  </form>
    <br><br>
    <hr />
{{/each}}